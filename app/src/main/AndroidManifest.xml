<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.kudusfood">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SotoKudus"
            android:label="Soto Kudus"/>
        <activity android:name=".LentogTanjung"
            android:label="Lentog Tanjung"/>
        <activity android:name=".NasiPindang"
            android:label="Nasi Pindang"/>
        <activity android:name=".PecelPakisColo"
            android:label="Pecel Pakis Colo"/>
        <activity android:name=".GarangAsem"
            android:label="Garang Asem"/>
        <activity android:name=".Profile"
            android:label="Saad Fauzi"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".Version"
            android:label="Version"
            android:parentActivityName=".MainActivity"/>
    </application>

</manifest>